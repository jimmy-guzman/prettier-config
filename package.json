{
  "name": "@jimmy.codes/prettier-config",
  "version": "1.6.0",
  "description": "My personal prettier config",
  "keywords": [
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-guzman/prettier-config.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --config ./dist/index.js --check --cache .",
    "format:fix": "prettier --config ./dist/index.js --write --cache .",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-ci || lefthook install"
  },
  "dependencies": {
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "0.6.14"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/node": "^24.3.0",
    "clean-pkg-json": "1.3.0",
    "commitlint": "19.8.1",
    "gitzy": "5.9.0",
    "is-ci": "4.1.0",
    "lefthook": "1.12.3",
    "prettier": "3.6.2",
    "publint": "0.3.12",
    "semantic-release": "24.2.7",
    "tsdown": "0.14.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "prettier": "^3.0"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">= 22.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitzy": {
    "useCommitlintConfig": true
  }
}
