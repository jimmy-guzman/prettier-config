{
  "name": "@jimmy.codes/prettier-config",
  "version": "2.0.2",
  "description": "My personal prettier config",
  "keywords": [
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-guzman/prettier-config.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --config ./dist/index.js --check --cache .",
    "format:fix": "prettier --config ./dist/index.js --write --cache .",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-ci || lefthook install"
  },
  "dependencies": {
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "0.7.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "20.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/node": "24.6.2",
    "clean-pkg-json": "1.3.0",
    "commitlint": "20.1.0",
    "gitzy": "5.9.0",
    "is-ci": "4.1.0",
    "lefthook": "1.13.6",
    "prettier": "3.6.2",
    "publint": "0.3.13",
    "semantic-release": "24.2.9",
    "tsdown": "0.15.6",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "prettier": "^3.0"
  },
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">= 22.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitzy": {
    "useCommitlintConfig": true
  }
}
